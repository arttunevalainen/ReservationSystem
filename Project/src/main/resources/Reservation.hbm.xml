<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="webproject.Models">
  <class name="Reservation" table="Reservations">
    <id column="Id" name="id">
      <generator class="increment"/>
    </id>
    <one-to-one cascade="save-update" class="webproject.Models.User" foreign-key="reserverid" name="reserver"/>
    <one-to-one cascade="save-update" class="webproject.Models.Reservable" foreign-key="reservableid" name="reservationItem"/>
    <property column="starttime" name="startTime" type="timestamp"/>
    <property column="endtime" name="endTime" type="timestamp"/>
  </class>
</hibernate-mapping>
